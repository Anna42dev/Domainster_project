												<input   style="margin-left: 15px;" type="radio"
													name="partBrand" id="partBrandNo" value="N" <%= (data.partBrand=='N') ? "checked" : "" %>>
												<label   for="partBrandNo">No</label>
										</div>
									</div>
									<div class="col-xs-5 col-sm-4 col-md-3 " style="height:110px"  >
