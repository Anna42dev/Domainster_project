<!doctype html>
<html lang="en">
	<% include ../partials/head %>

<body>
	<div class="image-container set-full-height" style="background-image: url('assets/img/wizard-city.jpg')">
		<!--   Webcotec Technology Branding   -->
		<!--   Big container   -->
		<div class="container">
			<div class="row" style="width: 95%; margin-left: auto;margin-right: auto; ">
				<div class="col-sm-12">
					<div class="wizard-container" style="margin-bottom:110px ;">
						<div class="card wizard-card" data-color="purple" id="wizard">
								<div class="row" style="padding: 20px;margin-left: 20px;margin-right: 20px;">
									<div class="col-sm-12">
										<h4 class="info-text h1text"> DOMAIN REGISTRATION</h4>
									</div>
									<div class="col-sm-6">
										<div class="form-group ">
											<label for="owner">OWNER*</label>
											<input type="text" class="form-control" value="<%= data.owner %>" placeholder="Enter Owner Name" required id="owner" name="owner">
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group ">
											<label for="ipAddress">IP*</label>
											<input type="text" class="form-control" id="ipAddress" value="<%= data.IP %>" required placeholder="Enter IPV4 Address"
												name="ipAddress">
											<span class="errorIp" aria-live="polite"></span>
										</div>
									</div>
									<div class="col-sm-12 ">
										<div class="form-group">
											<label for="acquired">ACQUIRED*</label>
											<input type="text" class="form-control" id="acquired" value="<%= data.acquired %>"  placeholder="Enter Date In Format (YYMMDD)" required name="acquired">
										</div>
									</div>
									<div class="col-xs-5 col-sm-4 col-md-3 " style="height:110px">
										<h4>ACTIVELY MARKETED</h4>
										<div class=" switch-field">
												<input   type="radio" name="actMarkt" id="actMarktYes" value="Y" <%= (data.actMarkt=='Y') ? "checked" : "" %>>
												<label   for="actMarktYes">Yes</label>
												<input   style="margin-left: 15px;" type="radio" name="actMarkt" id="actMarktNo" value="N" <%= (data.actMarkt=='N') ? "checked" : "" %>>
												<label   for="actMarktNo">No</label>
										</div>
									</div>
									<div class="col-xs-5 col-sm-4 col-md-3" style="height:110px" >
											<h4>BRAND PORTFOLIO</h4>
